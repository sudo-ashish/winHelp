.gsd/
.gemini/
.ag/
.agent/
.agents/
.ai/
.llm/
prompts/
sessions/
adapters/
model_capabilities.yaml

# ── Reference / backups (not runtime) ───────────────────────
eg-bak/
asset-bak/

# ── OS / System ──────────────────────────────────────────────
Thumbs.db
Desktop.ini
.DS_Store

# ── User-specific / local ────────────────────────────────────
user.config
local.settings.json
*.local.json

# ── Dev scratch files ────────────────────────────────────────
test.ps1
test-func.ps1
core.ps1
GSD-STYLE.md
PROJECT_RULES.md
